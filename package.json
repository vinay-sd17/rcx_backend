{
  "name": "rcx-api",
  "version": "1.0.0",
  "description": "RCX SVC API",
  "author": "Vinay S",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "dev-env-setup": "cp env/dev.env .env",
    "prod-env-setup": "cp env/prod.env .env",
    "nest-build": "nest build",
    "dev-no-truffle": "echo Use this build for DEV env && npm run dev-env-setup && nest start --watch",
    "dev": "echo Use this build for DEV env && npm run cleanup-truffle-build && npm run dev-env-setup && npm run truffle && npm run setup-truffle && nest start --watch",
    "deploy": "npm run start",
    "truffle": "mkdir -p dist && node_modules/.bin/truffle migrate",
    "truffle-build": "npm run truffle",
    "setup-truffle": "cp -r build dist/build",
    "cleanup-truffle-build": "rm -rf dist/build",
    "full-clean": "rm -rf dist && rm -rf build",
    "ebs-build": "echo Running as production mode && npm run prod-env-setup && npm run truffle-build && npm run setup-truffle && npm run nest-build",
    "start": "run_mode=prod node dist/src/main",
    "aws-start": "echo Use this build for testing ebs runner locally && npm run ebs-build && run_mode=prod node dist/src/main"
  },
  "dependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/mapped-types": "^0.2.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/swagger": "^4.7.13",
    "@nestjs/typeorm": "^7.1.5",
    "@truffle/hdwallet-provider": "^2.0.8",
    "@types/multer": "^1.4.5",
    "bcrypt": "^4.0.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.13.1",
    "convert-excel-to-json": "^1.7.0",
    "crypto-ts": "^1.0.2",
    "csv-batch": "^2.0.2",
    "eth-sig-util": "^3.0.0",
    "ethereumjs-util": "^7.0.5",
    "jimp": "^0.16.1",
    "logzio-nodejs": "^2.0.2",
    "mongodb": "^3.6.4",
    "multer": "^1.4.2",
    "multer-google-storage": "^1.3.0",
    "nest-access-control": "^2.0.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pdfkit": "^0.12.1",
    "pg": "^8.5.1",
    "prom-client": "^13.1.0",
    "qrcode": "^1.4.4",
    "qrcode-reader": "^1.0.4",
    "randomstring": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-stats": "^0.95.18",
    "swagger-ui-express": "^4.1.6",
    "truffle-contract": "^4.0.31",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.2",
    "web3": "^1.3.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.0.0",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/node": "^14.18.14",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "nodemon": "^2.0.6",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4",
    "truffle": "^5.5.15"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".spec.ts$",
    "testPathIgnorePatterns": [
      "mock.*"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
